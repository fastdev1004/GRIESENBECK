<Page x:Class="WpfApp.View.ReportDetailView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp.View"
      xmlns:converter="clr-namespace:WpfApp.Converter"
      mc:Ignorable="d" 
      d:DesignHeight="900" d:DesignWidth="1400"
      Title="ReportDetailView">

    <Page.Resources>
        <ResourceDictionary>
            <converter:BoolToVisibleConverter x:Key="BoolToVisibleConverter" />
            <converter:DateTimeToDateConverter x:Key="DateTimeToDateConverter" />
            <converter:DateTimeConverter x:Key="DateTimeConverter" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid>
            <Border BorderThickness="0 0 0 4" BorderBrush="#B6302D">
                <Grid Background="#14305F">
                    <Button Click="GoBack" Width="40" Height="20" HorizontalAlignment="Left" Margin="10,0,0,0">Back</Button>
                    <TextBlock Margin="80, 0,0,0" Text="Report View Detail" FontSize="32" Foreground="White" VerticalAlignment="Center">
                    </TextBlock>
                </Grid>
            </Border>
        </Grid>
        <Grid Grid.Row="1" Margin="10" Visibility="{Binding ActiveLaborListVisibility, Mode=TwoWay, Converter={StaticResource BoolToVisibleConverter}}">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="6*" />
                <ColumnDefinition Width="4*" />
            </Grid.ColumnDefinitions>
            <Grid>
                <TextBlock Style="{StaticResource ReportViewHeader}" Text="Active Labor List" />
            </Grid>
            <Grid Grid.Row="1" >
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <DataGrid ItemsSource="{Binding ReportActiveLabors, Mode=TwoWay}" GridLinesVisibility="None" AutoGenerateColumns="False" HeadersVisibility="Column" CanUserAddRows="False" BorderThickness="0" VirtualizingPanel.IsVirtualizing="False">
                    <DataGrid.Resources>
                        <Style TargetType="DataGridColumnHeader" BasedOn="{StaticResource GridColumnHeaderBase}">
                            <Setter Property="Background" Value="#C6D9F1" />
                            <Setter Property="Height" Value="Auto" />
                        </Style>
                        <Style TargetType="TextBox">
                            <Setter Property="BorderThickness" Value="0" />
                            <Setter Property="IsReadOnly" Value="True" />
                        </Style>
                        <Style TargetType="DataGridRow">
                            <Setter Property="Height" Value="Auto" />
                            <Setter Property="BorderThickness" Value="0" />
                        </Style>
                    </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTemplateColumn Width="*">
                            <DataGridTemplateColumn.CellTemplate >
                                <DataTemplate>
                                    <Border BorderBrush="Black" BorderThickness="0,2,0,0">
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <Border Background="#C6D9F1">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto" />
                                                        <RowDefinition Height="Auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="3*" />
                                                            <ColumnDefinition Width="2*" />
                                                            <ColumnDefinition Width="3*" />
                                                        </Grid.ColumnDefinitions>
                                                        <Grid>
                                                            <Grid.Resources>
                                                                <Style TargetType="TextBlock">
                                                                    <Setter Property="FontSize" Value="16" />
                                                                    <Setter Property="FontWeight" Value="Bold" />
                                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                                </Style>
                                                                <Style TargetType="TextBox">
                                                                    <Setter Property="FontSize" Value="16" />
                                                                    <Setter Property="FontWeight" Value="Bold" />
                                                                </Style>
                                                            </Grid.Resources>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="Auto" />
                                                                <ColumnDefinition Width="*" />
                                                            </Grid.ColumnDefinitions>
                                                            <Label Content="" />
                                                            <TextBlock Grid.Column="1" Text="{Binding ProjectName, Mode=TwoWay}" MouseLeftButtonDown="TextBlock_MouseLeftButtonDown" Tag="ProjectName" />
                                                        </Grid>
                                                        <Grid Grid.Column="1">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="Auto" />
                                                                <ColumnDefinition Width="*" />
                                                            </Grid.ColumnDefinitions>
                                                            <Label Content="Target Date:" />
                                                            <DatePicker Grid.Column="1" SelectedDate="{Binding TargetDate, Mode=TwoWay, Converter={StaticResource DateTimeConverter}}" VerticalAlignment="Center" Loaded="JobsTDate_Loaded" Tag="ActionLabor" />
                                                        </Grid>
                                                        <Grid Grid.Column="2">
                                                            <Button Content="View" Width="40" HorizontalAlignment="Left" Click="GoProject" CommandParameter="{Binding ProjectID}" />
                                                        </Grid>
                                                    </Grid>
                                                    <Grid Grid.Row="1">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="3*" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="1*" />
                                                        </Grid.ColumnDefinitions>
                                                        <WrapPanel>
                                                            <Label Content="Customer:" FontWeight="Bold" />
                                                            <TextBlock Text="{Binding CustomerName, Mode=TwoWay}" VerticalAlignment="Center" />
                                                        </WrapPanel>
                                                        <WrapPanel Grid.Column="1">
                                                            <Label Content="JobNo:" FontWeight="Bold" />
                                                            <TextBlock Text="{Binding JobNo, Mode=TwoWay}" VerticalAlignment="Center" />
                                                        </WrapPanel>
                                                        <WrapPanel Grid.Column="2">
                                                            <Label Content="Salesman:" FontWeight="Bold" />
                                                            <TextBlock Text="{Binding SalesmanName, Mode=TwoWay}" VerticalAlignment="Center" />
                                                        </WrapPanel>
                                                    </Grid>
                                                </Grid>
                                            </Border>
                                            <Grid Grid.Row="1">
                                                <DataGrid ItemsSource="{Binding LaborReports, Mode=TwoWay}" GridLinesVisibility="None" AutoGenerateColumns="False" HeadersVisibility="Column" CanUserAddRows="False" BorderThickness="0">
                                                    <DataGrid.Resources>
                                                        <Style TargetType="DataGridColumnHeader" BasedOn="{StaticResource GridColumnHeaderBase}">
                                                            <Setter Property="Background" Value="#C6D9F1" />
                                                            <Setter Property="Foreground" Value="Black" />
                                                            <Setter Property="Height" Value="25" />
                                                        </Style>
                                                    </DataGrid.Resources>
                                                    <DataGrid.Columns>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="2*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Target Date" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding TargetDate, Mode=TwoWay, Converter={StaticResource DateTimeToDateConverter}}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="3*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Scope Type" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding SovAcronym, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="4*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Labor" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding LaborDesc, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="1*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="CO #" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding CoItemNo, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="4*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Lab/Phase" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding LabPhase, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="1*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Compl" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <CheckBox IsChecked="{Binding Complete, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                    </DataGrid.Columns>
                                                </DataGrid>
                                            </Grid>
                                        </Grid>
                                    </Border>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                    </DataGrid.Columns>
                </DataGrid>
            </Grid>
            <StackPanel
      Grid.Row="2"
      Orientation="Horizontal"
      HorizontalAlignment="Center">
                <Button Margin="4,0" Content="&lt;&lt;" Command="{Binding FirstPageCommand}"/>
                <Button Margin="4,0" Content="&lt;" Command="{Binding PreivousPageCommand}"/>
                <StackPanel VerticalAlignment="Center" Orientation="Horizontal">
                    <TextBlock Text="{Binding Start}"/>
                    <TextBlock Text=" to "/>
                    <TextBlock Text="{Binding End}"/>
                    <TextBlock Text=" of "/>
                    <TextBlock Text="{Binding TotalItems}"/>
                </StackPanel>
                <Button Margin="4,0" Content="&gt;" Command="{Binding NextPageCommand}"/>
                <Button Margin="4,0" Content="&gt;&gt;" Command="{Binding LastPageCommand}" CommandParameter="{Binding ReportID, Mode=TwoWay}" />
            </StackPanel>
        </Grid>
    <Grid Grid.Row="1" Margin="10" Visibility="{Binding ActiveProjectListVisibility, Mode=TwoWay, Converter={StaticResource BoolToVisibleConverter}}">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="6*" />
                <ColumnDefinition Width="4*" />
            </Grid.ColumnDefinitions>
            <Grid>
                <TextBlock Style="{StaticResource ReportViewHeader}" Text="Active Projects List" />
            </Grid>
            <Grid Grid.Row="1" >
                <DataGrid ItemsSource="{Binding ReportActiveProjects, Mode=TwoWay}" GridLinesVisibility="None" AutoGenerateColumns="False" HeadersVisibility="Column" CanUserAddRows="False" BorderThickness="0" VirtualizingPanel.IsVirtualizing="False">
                    <DataGrid.Resources>
                        <Style TargetType="DataGridColumnHeader" BasedOn="{StaticResource GridColumnHeaderBase}">
                            <Setter Property="Background" Value="#C6D9F1" />
                            <Setter Property="Height" Value="Auto" />
                        </Style>
                        <Style TargetType="TextBox">
                            <Setter Property="BorderThickness" Value="0" />
                            <Setter Property="IsReadOnly" Value="True" />
                        </Style>
                        <Style TargetType="DataGridRow">
                            <Setter Property="Height" Value="Auto" />
                            <Setter Property="BorderThickness" Value="0" />
                        </Style>
                    </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTemplateColumn Width="*">
                            <DataGridTemplateColumn.CellTemplate >
                                <DataTemplate>
                                    <Border BorderBrush="Black" BorderThickness="0,2,0,0">
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"/>
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <Border Background="#C6D9F1">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="3*" />
                                                            <ColumnDefinition Width="2*" />
                                                            <ColumnDefinition Width="3*" />
                                                        </Grid.ColumnDefinitions>
                                                        <Grid>
                                                            <Grid.Resources>
                                                                <Style TargetType="TextBlock">
                                                                    <Setter Property="FontSize" Value="16" />
                                                                    <Setter Property="FontWeight" Value="Bold" />
                                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                                </Style>
                                                                <Style TargetType="TextBox">
                                                                    <Setter Property="FontSize" Value="16" />
                                                                    <Setter Property="FontWeight" Value="Bold" />
                                                                </Style>
                                                            </Grid.Resources>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="Auto" />
                                                                <ColumnDefinition Width="*" />
                                                            </Grid.ColumnDefinitions>
                                                            <Label Content="" />
                                                            <TextBlock Grid.Column="1" Text="{Binding ProjectName, Mode=TwoWay}" MouseLeftButtonDown="TextBlock_MouseLeftButtonDown" Tag="ProjectName" />
                                                        </Grid>
                                                        <Grid Grid.Column="1">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="Auto" />
                                                                <ColumnDefinition Width="*" />
                                                            </Grid.ColumnDefinitions>
                                                            <Label Content="Target Date:" />
                                                            <DatePicker Grid.Column="1" SelectedDate="{Binding TargetDate, Mode=TwoWay, Converter={StaticResource DateTimeConverter}}" VerticalAlignment="Center" Loaded="JobsTDate_Loaded" Tag="ActiveProject" />
                                                        </Grid>
                                                        <Grid Grid.Column="2">
                                                            <Button Content="View" Width="40" HorizontalAlignment="Left" Click="GoProject" CommandParameter="{Binding ProjectID}" />
                                                        </Grid>
                                                    </Grid>
                                                    <Grid Grid.Row="1">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="3*" />
                                                            <ColumnDefinition Width="1*" />
                                                            <ColumnDefinition Width="1*" />
                                                        </Grid.ColumnDefinitions>
                                                        <WrapPanel>
                                                            <Label Content="Customer:" FontWeight="Bold" />
                                                            <TextBlock Text="{Binding CustomerName, Mode=TwoWay}" VerticalAlignment="Center" />
                                                        </WrapPanel>
                                                        <WrapPanel Grid.Column="1">
                                                            <Label Content="JobNo:" FontWeight="Bold" />
                                                            <TextBlock Text="{Binding JobNo, Mode=TwoWay}" VerticalAlignment="Center" />
                                                        </WrapPanel>
                                                        <WrapPanel Grid.Column="2">
                                                            <Label Content="Salesman:" FontWeight="Bold" />
                                                            <TextBlock Text="{Binding SalesmanName, Mode=TwoWay}" VerticalAlignment="Center" />
                                                        </WrapPanel>
                                                    </Grid>
                                                </Grid>
                                            </Border>
                                            <Grid Grid.Row="1">
                                                <DataGrid ItemsSource="{Binding ProjectMatTrackings, Mode=TwoWay}" GridLinesVisibility="None" AutoGenerateColumns="False" HeadersVisibility="Column" CanUserAddRows="False" BorderThickness="0">
                                                    <DataGrid.Resources>
                                                        <Style TargetType="DataGridColumnHeader" BasedOn="{StaticResource GridColumnHeaderBase}">
                                                            <Setter Property="Background" Value="#C6D9F1" />
                                                            <Setter Property="Foreground" Value="Black" />
                                                            <Setter Property="Height" Value="25" />
                                                        </Style>
                                                    </DataGrid.Resources>
                                                    <DataGrid.Columns>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="2*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Matl Reqd" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding MatReqdDate, Mode=TwoWay, Converter={StaticResource DateTimeToDateConverter}}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="3*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Manufacturer" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding ManufName, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="1*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="CO #" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding ManuOrderNo, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="4*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Phase" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding MatPhase, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="4*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Material" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding MatDesc, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="4*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="R.F.F" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding RFF, Mode=TwoWay, Converter={StaticResource DateTimeToDateConverter}}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="4*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Sched.Ship" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <TextBlock Text="{Binding SchedShip, Mode=TwoWay, Converter={StaticResource DateTimeToDateConverter}}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                        <DataGridTemplateColumn IsReadOnly="False" Width="1*">
                                                            <DataGridTemplateColumn.Header>
                                                                <TextBlock Text="Compl" TextWrapping="Wrap" FontWeight="Bold" />
                                                            </DataGridTemplateColumn.Header>
                                                            <DataGridTemplateColumn.CellTemplate>
                                                                <DataTemplate>
                                                                    <CheckBox IsChecked="{Binding OrderComplete, Mode=TwoWay}" />
                                                                </DataTemplate>
                                                            </DataGridTemplateColumn.CellTemplate>
                                                        </DataGridTemplateColumn>
                                                    </DataGrid.Columns>
                                                </DataGrid>
                                            </Grid>
                                        </Grid>
                                    </Border>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                    </DataGrid.Columns>
                </DataGrid>
            </Grid>
            <StackPanel
      Grid.Row="2"
      Orientation="Horizontal"
      HorizontalAlignment="Center">
                <Button Margin="4,0" Content="&lt;&lt;" Command="{Binding FirstPageCommand}"/>
                <Button Margin="4,0" Content="&lt;" Command="{Binding PreivousPageCommand}"/>
                <StackPanel VerticalAlignment="Center" Orientation="Horizontal">
                    <TextBlock Text="{Binding Start}"/>
                    <TextBlock Text=" to "/>
                    <TextBlock Text="{Binding End}"/>
                    <TextBlock Text=" of "/>
                    <TextBlock Text="{Binding TotalItems}"/>
                </StackPanel>
                <Button Margin="4,0" Content="&gt;" Command="{Binding NextPageCommand}"/>
                <Button Margin="4,0" Content="&gt;&gt;" Command="{Binding LastPageCommand}" CommandParameter="{Binding ReportID, Mode=TwoWay}" />
            </StackPanel>
        </Grid>

<Grid Grid.Row="1" Margin="10" Visibility="{Binding ApprovedVisibility, Mode=TwoWay, Converter={StaticResource BoolToVisibleConverter}}">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto" /> 
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="6*" />
                <ColumnDefinition Width="4*" />
            </Grid.ColumnDefinitions>
            <Grid>
                <TextBlock Style="{StaticResource ReportViewHeader}" Text="Approved, Not Released" />
            </Grid>
            <Grid Grid.Row="1" >
                <DataGrid ItemsSource="{Binding ReportApproveNotReleases, Mode=TwoWay}" GridLinesVisibility="None" AutoGenerateColumns="False" HeadersVisibility="Column" BorderThickness="0" Background="White">
                    <DataGrid.Resources>
                        <Style TargetType="DataGridColumnHeader" BasedOn="{StaticResource GridColumnHeaderBase}">
                            <Setter Property="Background" Value="White" />
                            <Setter Property="Foreground" Value="Black" />
                        </Style>
                        <Style TargetType="TextBox">
                            <Setter Property="BorderThickness" Value="0,0,0,1" />
                        </Style>
                        <Style TargetType="ComboBox">
                            <Setter Property="BorderThickness" Value="0,0,0,1" />
                            <Setter Property="IsEditable" Value="True" />
                        </Style>
                    </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTemplateColumn IsReadOnly="False" Width="2*">
                            <DataGridTemplateColumn.Header>
                                <Border BorderBrush="Black" BorderThickness="0,0,0,1">
                                    <TextBlock Text="Mat Req Date" TextWrapping="Wrap" />
                                </Border>
                            </DataGridTemplateColumn.Header>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding MatReqDate , Converter={StaticResource DateTimeToDateConverter}}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn IsReadOnly="False" Width="1*">
                            <DataGridTemplateColumn.Header>
                                <Border BorderBrush="Black" BorderThickness="0,0,0,1">
                                    <TextBlock Text="Job No" TextWrapping="Wrap" />
                                </Border>
                            </DataGridTemplateColumn.Header>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding JobNo}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn IsReadOnly="False" Width="4*">
                            <DataGridTemplateColumn.Header>
                                <Border BorderBrush="Black" BorderThickness="0,0,0,1">
                                    <TextBlock Text="Project" TextWrapping="Wrap" />
                                </Border>
                            </DataGridTemplateColumn.Header>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding ProjectName}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn IsReadOnly="False" Width="3*">
                            <DataGridTemplateColumn.Header>
                                <Border BorderBrush="Black" BorderThickness="0,0,0,1">
                                    <TextBlock Text="Salesman" TextWrapping="Wrap" />
                                </Border>
                            </DataGridTemplateColumn.Header>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding SalesmanName}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn IsReadOnly="False" Width="3*">
                            <DataGridTemplateColumn.Header>
                                <Border BorderBrush="Black" BorderThickness="0,0,0,1">
                                    <TextBlock Text="Material" TextWrapping="Wrap" />
                                </Border>
                            </DataGridTemplateColumn.Header>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding MaterialName}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn IsReadOnly="False" Width="3*">
                            <DataGridTemplateColumn.Header>
                                <Border BorderBrush="Black" BorderThickness="0,0,0,1">
                                    <TextBlock Text="Manufacturer" TextWrapping="Wrap" />
                                </Border>
                            </DataGridTemplateColumn.Header>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding ManufName}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn IsReadOnly="False" Width="3*">
                            <DataGridTemplateColumn.Header>
                                <Border BorderBrush="Black" BorderThickness="0,0,0,1">
                                    <TextBlock Text="Subm Appr" TextWrapping="Wrap" />
                                </Border>
                            </DataGridTemplateColumn.Header>
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding SubmAppr, Converter={StaticResource DateTimeToDateConverter}}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
